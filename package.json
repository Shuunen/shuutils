{
  "name": "shuutils",
  "version": "3.4.0",
  "author": {
    "name": "Romain Racamier-Lafon",
    "email": "romain.racamier@gmail.com"
  },
  "description": "utils collection",
  "engines": {
    "node": ">=8.12.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "check-all": "run-p verify lint test",
    "dev": "run-s build test:watch",
    "lint": "standard --fix src/**/*.ts test/**/*.ts",
    "prepublish": "run-s check-all build",
    "pretest": "run-p lint",
    "test": "ava",
    "test:watch": "ava --watch",
    "verify": "nsp check",
    "version:major": "run-s version:increment:major version:publish",
    "version:minor": "run-s version:increment:minor version:publish",
    "version:patch": "run-s version:increment:patch version:publish",
    "version:increment:major": "yarn version --major",
    "version:increment:minor": "yarn version --minor",
    "version:increment:patch": "yarn version --patch",
    "version:publish": "yarn publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/shuutils.git"
  },
  "keywords": [
    "shuunen",
    "utils",
    "collection",
    "pure",
    "javascript",
    "typescript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shuunen/shuutils/issues"
  },
  "homepage": "https://github.com/Shuunen/shuutils#readme",
  "private": false,
  "devDependencies": {
    "ava": "1.0.0-beta.8",
    "eslint-plugin-typescript": "0.13.0",
    "npm-run-all": "4.1.3",
    "nsp": "3.2.1",
    "standard": "12.0.1",
    "ts-node": "7.0.1",
    "typescript": "3.1.6",
    "typescript-eslint-parser": "20.1.1"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "plugins": [
      "typescript"
    ]
  }
}
