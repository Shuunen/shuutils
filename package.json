{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/shuutils/issues",
  "description": "utils collection",
  "devDependencies": {
    "@types/node": "16",
    "@typescript-eslint/eslint-plugin": "5",
    "@typescript-eslint/parser": "5",
    "c8": "7",
    "eslint": "8",
    "eslint-plugin-unicorn": "39",
    "jsdom": "19",
    "jsdom-global": "3",
    "npm-run-all": "4",
    "repo-check": "^1.21",
    "tsm": "2",
    "typescript": "4",
    "uvu": "0",
    "watchlist": "0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/Shuunen/shuutils#readme",
  "keywords": [
    "shuunen",
    "utils",
    "collection",
    "pure",
    "javascript",
    "typescript"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "shuutils",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/shuutils.git"
  },
  "scripts": {
    "build": "rm dist -rf && tsc",
    "check": "repo-check",
    "ci": "run-s build lint check test",
    "dev": "run-s build test:watch",
    "lint": "eslint --fix --ignore-path .gitignore --ext .js,.ts .",
    "postversion": "git push && git push --tags && npm publish",
    "preversion": "npm run ci",
    "start": "npm run dev",
    "test": "c8 uvu -r tsm -r jsdom-global/register tests",
    "test:watch": "watchlist src tests -- npm test"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "5.4.0"
}
