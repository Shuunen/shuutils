{
  "name": "shuutils",
  "version": "2.0.2",
  "author": {
    "name": "Romain Racamier-Lafon",
    "email": "romain.racamier@gmail.com"
  },
  "description": "utils collection",
  "engines": {
    "node": ">=8.11.4"
  },
  "main": "dist/_index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel -d dist/ src/",
    "check-all": "run-p verify lint test",
    "dev": "yarn test --watch",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "precommit": "lint-staged",
    "prepublish": "run-s check-all build",
    "pretest": "run-p lint",
    "test": "ava",
    "verify": "nsp check",
    "version:major": "run-s version:increment:major version:publish",
    "version:minor": "run-s version:increment:minor version:publish",
    "version:patch": "run-s version:increment:patch version:publish",
    "version:increment:major": "npm version major",
    "version:increment:minor": "npm version minor",
    "version:increment:patch": "npm version patch",
    "version:publish": "yarn publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/shuutils.git"
  },
  "keywords": [
    "shuunen",
    "utils",
    "collection",
    "pure",
    "javascript"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Shuunen/shuutils/issues"
  },
  "homepage": "https://github.com/Shuunen/shuutils#readme",
  "private": false,
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0-beta.51",
    "@babel/runtime": "^7.0.0-beta.51",
    "ava": "1.0.0-beta.6",
    "babel-eslint": "8",
    "eslint": "4.x",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "nsp": "^3.2.1"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
