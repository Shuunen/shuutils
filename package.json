    "postversion": "git push && git push --tags && npm publish",
    "preversion": "npm run ci",
    "prebuild": "rimraf dist",
    "start": "npm run dev",
    "test": "nyc mocha",
    "test:watch": "onchange --initial \"src/**/*.ts\" \"tests/**/*.ts\" -- npm test",
