{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "test/*.ts"
    ]
  },
  "bugs": "https://github.com/Shuunen/shuutils/issues",
  "description": "utils collection",
  "dependencies": {
    "dlv": "1.1.3"
  },
  "devDependencies": {
    "@types/dlv": "1.1.2",
    "@types/node": "14.14.16",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "ava": "3.14.0",
    "eslint": "7.16.0",
    "eslint-config-standard": "16.0.2",
    "eslint-config-standard-with-typescript": "19.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-unicorn": "25.0.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "repo-check": "1.14.2",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "files": [
    "dist/src"
  ],
  "homepage": "https://github.com/Shuunen/shuutils#readme",
  "keywords": [
    "shuunen",
    "utils",
    "collection",
    "pure",
    "javascript",
    "typescript"
  ],
  "license": "MIT",
  "main": "dist/src/index.js",
  "name": "shuutils",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/shuutils.git"
  },
  "scripts": {
    "build": "tsc",
    "check": "repo-check",
    "ci": "run-s lint test check",
    "dev": "run-s build test:watch",
    "lint": "eslint --fix --ignore-path .gitignore --ext .ts .",
    "postversion": "git push && git push --tags && npm publish",
    "pretest": "npm run build",
    "preversion": "npm run ci",
    "prebuild": "rimraf dist",
    "start": "npm run dev",
    "test": "nyc ava",
    "test:watch": "ava --watch",
    "update": "npx npm-check-updates -u",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch"
  },
  "types": "dist/index.d.ts",
  "version": "4.1.0"
}
