{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "bugs": "https://github.com/Shuunen/shuutils/issues",
  "description": "utils collection",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.4.0",
    "@typescript-eslint/parser": "2.4.0",
    "ava": "2.4.0",
    "npm-run-all": "4.1.5",
    "standardx": "5.0.0",
    "ts-node": "8.4.1",
    "typescript": "3.6.4"
  },
  "engines": {
    "node": ">=8.12.0"
  },
  "eslintConfig": {
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "no-unused-vars": "off"
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/Shuunen/shuutils#readme",
  "keywords": [
    "shuunen",
    "utils",
    "collection",
    "pure",
    "javascript",
    "typescript"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "shuutils",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/shuutils.git"
  },
  "scripts": {
    "build": "tsc",
    "check": "npx repo-check",
    "ci": "run-s check lint build test",
    "dev": "run-s build test:watch",
    "lint": "standardx **/*.ts --fix",
    "postversion": "npm run ci && npm publish",
    "start": "npm run dev",
    "test": "ava",
    "test:watch": "ava --watch",
    "update": "npx npm-check-updates -u",
    "version:major": "npm version major",
    "version:minor": "npm version minor",
    "version:patch": "npm version patch"
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "types": "dist/index.d.ts",
  "version": "3.4.0"
}
