{
  "name": "shuutils",
  "version": "2.0.4",
  "author": {
    "name": "Romain Racamier-Lafon",
    "email": "romain.racamier@gmail.com"
  },
  "description": "utils collection",
  "engines": {
    "node": ">=8.11.4"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "check-all": "run-p verify lint test",
    "dev": "yarn test --watch",
    "lint:fix": "run-s lint --fix",
    "lint": "tslint -c tslint.json '{src,test}/**/*.ts'",
    "precommit": "lint-staged",
    "prepublish": "run-s check-all build",
    "pretest": "run-p lint",
    "test": "ava",
    "verify": "nsp check",
    "version:major": "run-s version:increment:major version:publish",
    "version:minor": "run-s version:increment:minor version:publish",
    "version:patch": "run-s version:increment:patch version:publish",
    "version:increment:major": "npm version major",
    "version:increment:minor": "npm version minor",
    "version:increment:patch": "npm version patch",
    "version:publish": "yarn publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/shuutils.git"
  },
  "keywords": [
    "shuunen",
    "utils",
    "collection",
    "pure",
    "javascript",
    "typescript"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Shuunen/shuutils/issues"
  },
  "homepage": "https://github.com/Shuunen/shuutils#readme",
  "private": false,
  "devDependencies": {
    "ava": "1.0.0-beta.8",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "nsp": "^3.2.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
