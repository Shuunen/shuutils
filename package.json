  },
  "devDependencies": {
    "@types/dlv": "1.1.2",
    "@types/mocha": "8.2.0",
    "@types/node": "14.14.25",
    "mocha": "8.2.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "onchange": "7.1.0",
    "repo-check": "1.14.8",
    "rimraf": "3.0.2",
    "ts-node": "9.1.1",
    "typescript": "4.1.4",
    "xo": "0.37.1"
  },
  "files": [
    "dist/src"
  ],
  "homepage": "https://github.com/Shuunen/shuutils#readme",
  "keywords": [
    "shuunen",
    "utils",
    "collection",
    "pure",
    "javascript",
    "typescript"
  ],
  "license": "MIT",
  "main": "dist/src/index.js",
  "name": "shuutils",
