{
  "$schema": "https://json.schemastore.org/package",
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bin": {
    "unique-mark": "./dist/unique-mark.js"
  },
  "bugs": "https://github.com/Shuunen/shuutils/issues",
  "dependencies": {
    "tiny-glob": "0",
    "uvu": "0"
  },
  "description": "utils collection",
  "devDependencies": {
    "@types/node": "18",
    "c8": "7",
    "dependency-cruiser": "12",
    "esbuild": "0.16",
    "eslint": "8.31",
    "eslint-config-hardcore": "26",
    "eslint-plugin-jsdoc": "39",
    "jsdom": "20",
    "jsdom-global": "3",
    "repo-check": "1.29",
    "tsm": "2",
    "typescript": "4",
    "watchlist": "0"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/Shuunen/shuutils#readme",
  "keywords": [
    "shuunen",
    "utils",
    "collection",
    "pure",
    "javascript",
    "typescript"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "name": "shuutils",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shuunen/shuutils.git"
  },
  "scripts": {
    "build": "rm dist -rf && tsc --emitDeclarationOnly && node bin/build && echo build success",
    "build:watch": "watchlist bin src -eager -- npm run build",
    "check": "repo-check",
    "ci": "npm run build && npm run lint && npm run check && npm run test && echo ci success",
    "dev": "npm run test:watch",
    "lint": "tsc --noEmit && eslint --cache --fix --ignore-path .gitignore --ext .js,.ts . && depcruise src --config && echo lint success",
    "lint:watch": "watchlist src tests -eager -- npm run lint",
    "postversion": "git push && git push --tags && npm publish",
    "prepack": "npm run build",
    "preversion": "npm run ci",
    "start": "npm run dev",
    "test": "c8 uvu -r tsm -r jsdom-global/register tests && echo test success",
    "test:watch": "watchlist src tests -eager -- npm test"
  },
  "type": "module",
  "types": "dist/src/index.d.ts",
  "version": "7.1.0"
}
